{
  "builds": [
       {
         "src": "your_django_project/wsgi.py",
         "use": "@vercel/python"
       }
     ],
  "routes": [
     {
         "src": "/static/(.*)",
         "dest": "/static/$1"
       },

       {
         "src": "/(.*)",
         "dest": "your_django_project/wsgi.py"
       }
  ]
}
